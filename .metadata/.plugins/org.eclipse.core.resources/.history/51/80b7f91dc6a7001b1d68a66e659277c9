
public class FIZZ {

	public static void main(String[] args) { 
		
		
				FIZZ fz = new FIZZ();
				for(int count = 1; count <= 50; count++) {
					System.out.println(fz.getFizzBuzz(count));
				}
			}

		    String getFizzBuzz(int count) {
		    	if(count%3 == 0 && count % 5 ==0) {
		    		return count + " FizzBuzz ";
		    	}else if(count %3 == 0) {
		    }
		    	return count + " Fizz ";
		    }
		    else if(count %5 ==0) {
		    	return count + " Buzz ";
		    }else
		    {
		    	return count + "";
		    }
		}

		}

		// TODO Auto-generated method stub

	}

}
